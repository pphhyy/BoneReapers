<!-- =================== Head Gene Defs =================== -->	
	
	<GeneDef ParentName="GeneJawBase" Name="pphhyy_BoneReaperGeneHeadBase" Abstract="True">
		<displayCategory>Miscellaneous</displayCategory>
		<exclusionTags>
      		<li>BeardStyle</li>
    	</exclusionTags>
		<beardTagFilter>
      		<whitelist>true</whitelist>
      		<tags>
        		<li>NoBeard</li>
				<li>HairStyle</li>
      		</tags>
    	</beardTagFilter>
    	<hairTagFilter>
      		<whitelist>true</whitelist>
      		<tags>
        		<li>Bald</li>
      		</tags>
    	</hairTagFilter>		
		<modExtensions>
		 <li Class="BetterPrerequisites.GeneExtension">
		 <disableFacialAnimations>true</disableFacialAnimations>
		 </li>
	    </modExtensions>
	<renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPaths>
          <li>Pawns/Apokryphos/ApokryphosHead</li>
        </texPaths>
        <useSkinShader>false</useSkinShader>
        <colorType>Skin</colorType>
        <drawData>
          <defaultData>
          <layer>52</layer>
          </defaultData>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
	</GeneDef>

